                },
                "biz_data": {
                    "angle": 30.0,
                    "roll_angle": 30.0
                }
            }
        ],
        "rule":[
            {
                "type": "data_duplicate",
                "brief": "数据去重",
                "value": [
                  "$item_data.id"
                ]
          }
      ]
    },
    "biz" : [
        {
            "brief": "设置图层优先级",
            "type": "biz_set_priority",
            "value": {
                "draw_priority": 140,
                "sub_priority": 3815
            }
        },
        {
          "brief": "设置显示比例尺范围",
          "type": "biz_set_display_scale",
          "value": {
            "min_scale": 15,
            "max_scale": 19
          }
        },
        {
            "brief": "设置天空盒裁剪开关",
            "type": "biz_set_clip_sky",
            "value" : false
        }
    ],
    "style" : {
        "marker_info" : {
        },
        "style_info" : {
            "point_car_meeting_warn_head_day_left": {
                "point_layer_item_style": {
                    "normal_style": {
                        "poi_marker_id": "car_day_left",
                        "poi_marker_info":{
                            "anchor": 4
                        }
                    }
                }
            },
            "point_car_meeting_warn_head_day_right": {
                "point_layer_item_style": {
                    "normal_style": {
                        "poi_marker_id": "car_day_right",
                        "poi_marker_info":{
                            "anchor": 4
                        }
                    }
                }
            },
            "point_car_meeting_warn_head_night_left": {
                "point_layer_item_style": {
                    "normal_style": {
                        "poi_marker_id": "car_night_left",
                        "poi_marker_info":{
                            "anchor": 4
                        }
                    }
                }
            },
            "point_car_meeting_warn_head_night_right": {
                "point_layer_item_style": {
                    "normal_style": {
                        "poi_marker_id": "car_night_right",
                        "poi_marker_info":{
                            "anchor": 4
                        }
                    }
                }
            }
        },
        "style_name" : {
            "prefix_name" : "point_car_meeting_warn_head",
            "rule" : [
                {
                    "brief": "日夜样式if动态规则",
                    "type": "style_if",
                    "value": {
                    "condition": {
                        "func": {
                            "name": "IsNightMode"
                        },
                        "operator": "equal",
                        "value": true
                    },
                    "case": {
                        "true": {
                            "if": {
                                "condition": {
                                "all": [
                                    {
                                        "fact": "$biz_data.roll_angle",
                                        "operator": "ge",
                                        "value": 0.0
                                    },
                                    {
                                        "fact": "$biz_data.roll_angle",
                                        "operator": "lt",
                                        "value": 180.0
                                    }
                                ]
                            },
                            "case": {
                                "true": "_night_right",
                                "false": "_night_left"
                            }
                        }
                        },
                        "false": {
                            "if": {
                                "condition": {
                                "all": [
                                    {
                                        "fact": "$biz_data.roll_angle",
                                        "operator": "ge",
                                        "value": 0.0
                                    },
                                    {
                                        "fact": "$biz_data.roll_angle",
                                        "operator": "lt",
                                        "value": 180.0
                                    }
                                ]
                            },
                            "case": {
                                "true": "_day_right",
                                "false": "_day_left"
                            }
                            }
                        }
                        }
                    }
                }
            ]
        }
    }
}