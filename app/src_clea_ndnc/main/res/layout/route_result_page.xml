<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />

        <variable
            name="ViewModel"
            type="com.sgm.navi.hmi.route.RouteViewModel" />
    </data>

    <com.sgm.navi.ui.view.SkinConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="@{ViewModel.getIncludePageVisibility() == 0 ? View.VISIBLE : View.GONE}"
        android:clickable="false">

        <com.sgm.navi.ui.view.SkinConstraintLayout
            android:id="@+id/route_line_info_title"
            android:layout_width="@dimen/dp_613"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_shadow_navi_ndlc_8155"
            android:minHeight="@dimen/dp_163"
            android:paddingHorizontal="@dimen/dp_34"
            android:paddingVertical="@dimen/dp_34"
            android:clickable="true">

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/route_line_info_img_close"
                clickCommand="@{ViewModel.getCloseRouteClick()}"
                android:layout_width="@dimen/dp_49"
                android:layout_height="@dimen/dp_49"
                android:layout_marginStart="@dimen/dp_12"
                android:layout_marginTop="@dimen/dp_23"
                android:layout_marginBottom="@dimen/dp_23"
                android:src="@drawable/img_route_back_sw1225"
                android:contentDescription="@string/app_currency_back"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/route_line_info_tv_search_waypoint"
                clickCommand="@{ViewModel.getAlongWaySearchClick()}"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/sp_36"
                android:layout_marginTop="@dimen/dp_31"
                android:layout_marginEnd="@dimen/dp_25"
                android:layout_marginBottom="@dimen/dp_31"
                android:gravity="center"
                android:text="@string/route_along_search"
                android:textColor="@color/text_route_defult"
                android:textSize="@dimen/sp_26"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/route_line_info_img_search_waypoint"
                clickCommand="@{ViewModel.getAlongWaySearchClick()}"
                android:layout_width="@dimen/dp_36"
                android:layout_height="@dimen/dp_36"
                android:layout_marginEnd="@dimen/dp_7"
                android:src="@drawable/img_route_add_point_sw1225"
                app:layout_constraintBottom_toBottomOf="@+id/route_line_info_tv_search_waypoint"
                app:layout_constraintEnd_toStartOf="@id/route_line_info_tv_search_waypoint"
                app:layout_constraintTop_toTopOf="@+id/route_line_info_tv_search_waypoint" />

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/route_line_cicle_start_bg"
                android:layout_width="@dimen/dp_9"
                android:layout_height="@dimen/dp_80"
                android:visibility="@{ViewModel.getViaPoiListVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintStart_toEndOf="@id/route_line_info_img_close"
                app:layout_constraintTop_toTopOf="parent" />

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/route_item_via_start_img"
                android:layout_width="@dimen/dp_9"
                android:layout_height="@dimen/dp_9"
                android:src="@drawable/circle_dot_green"
                android:visibility="@{ViewModel.getViaPoiListVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintBottom_toBottomOf="@+id/route_line_info_img_close"
                app:layout_constraintStart_toStartOf="@id/route_line_cicle_start_bg"
                app:layout_constraintTop_toTopOf="@+id/route_line_info_img_close" />

            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="0.5dp"
                android:layout_height="0dp"
                android:background="@color/route_line_color"
                android:visibility="@{ViewModel.getViaPoiListVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintBottom_toBottomOf="@+id/route_line_cicle_start_bg"
                app:layout_constraintEnd_toEndOf="@id/route_line_cicle_start_bg"
                app:layout_constraintStart_toStartOf="@id/route_line_cicle_start_bg"
                app:layout_constraintTop_toBottomOf="@+id/route_item_via_start_img" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/route_line_tv_title"
                android:layout_width="@dimen/dp_336"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_29"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:maxLines="1"
                android:text="@{ViewModel.getTitle()}"
                android:textColor="@color/text_route_detail_title"
                android:textSize="@dimen/sp_29"
                app:layout_constraintStart_toEndOf="@id/route_line_cicle_start_bg"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="我的位置" />

            <com.sgm.navi.ui.view.SkinConstraintLayout
                android:id="@+id/route_line_via_poi_recycle_bg"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:visibility="@{ViewModel.getViaPoiListVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintEnd_toEndOf="@+id/route_line_info_tv_search_waypoint"
                app:layout_constraintStart_toEndOf="@+id/skinTextView2"
                app:layout_constraintStart_toStartOf="@id/route_item_via_start_img"
                app:layout_constraintTop_toBottomOf="@+id/route_line_cicle_start_bg">

                <com.sgm.navi.ui.view.SkinRecyclerView
                    android:id="@+id/route_line_via_poi_recycle"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingTop="-24dp"
                    android:visibility="@{ViewModel.getViaPoiListAllVisibility() ? View.VISIBLE : View.GONE}"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:visibility="gone" />

                <com.sgm.navi.ui.view.SkinView
                    android:id="@+id/line_bg"
                    android:layout_width="@dimen/dp_9"
                    android:layout_height="@dimen/dp_48"
                    android:visibility="@{ViewModel.getViaPoiListAllVisibility() ? View.GONE : View.VISIBLE}" />

                <com.sgm.navi.ui.view.SkinImageView
                    android:id="@+id/route_item_via_img"
                    android:layout_width="@dimen/dp_9"
                    android:layout_height="@dimen/dp_9"
                    android:src="@drawable/circle_dot_orange"
                    android:visibility="@{ViewModel.getViaPoiListAllVisibility() ? View.GONE : View.VISIBLE}"
                    app:layout_constraintBottom_toBottomOf="@+id/line_bg"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/line_bg" />

                <com.sgm.navi.ui.view.SkinTextView
                    android:layout_width="0.5dp"
                    android:layout_height="@dimen/dp_16"
                    android:background="@color/route_line_color"
                    android:visibility="@{ViewModel.getViaPoiListAllVisibility() ? View.GONE : View.VISIBLE}"
                    app:layout_constraintEnd_toEndOf="@+id/line_bg"
                    app:layout_constraintStart_toStartOf="@+id/line_bg"
                    app:layout_constraintTop_toBottomOf="@+id/route_item_via_start_img" />

                <com.sgm.navi.ui.view.SkinTextView
                    android:layout_width="0.5dp"
                    android:layout_height="@dimen/dp_16"
                    android:background="@color/route_line_color"
                    android:visibility="@{ViewModel.getViaPoiListAllVisibility() ? View.GONE : View.VISIBLE}"
                    app:layout_constraintEnd_toEndOf="@+id/line_bg"
                    app:layout_constraintStart_toStartOf="@+id/line_bg"
                    app:layout_constraintTop_toBottomOf="@+id/route_item_via_img" />

                <com.sgm.navi.ui.view.SkinTextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/dp_20"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="@{ViewModel.getParamDes()}"
                    android:textColor="@color/text_route_defult"
                    android:textSize="@dimen/sp_26"
                    android:visibility="@{ViewModel.getViaPoiListAllVisibility() ? View.GONE : View.VISIBLE}"
                    app:layout_constraintBottom_toBottomOf="@+id/line_bg"
                    app:layout_constraintStart_toEndOf="@+id/line_bg"
                    app:layout_constraintTop_toTopOf="@+id/line_bg"
                    tools:text="途径2地：aaa，bbb" />

            </com.sgm.navi.ui.view.SkinConstraintLayout>

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/route_line_cicle_end_bg"
                android:layout_width="@dimen/dp_9"
                android:layout_height="@dimen/dp_60"
                android:visibility="@{ViewModel.getViaPoiListVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintStart_toStartOf="@id/route_line_cicle_start_bg"
                app:layout_constraintTop_toBottomOf="@id/route_line_via_poi_recycle_bg" />

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/route_item_via_endimg"
                android:layout_width="@dimen/dp_9"
                android:layout_height="@dimen/dp_9"
                android:src="@drawable/circle_dot_red"
                android:visibility="@{ViewModel.getViaPoiListVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintBottom_toBottomOf="@+id/route_line_cicle_end_bg"
                app:layout_constraintStart_toStartOf="@+id/route_line_cicle_end_bg"
                app:layout_constraintTop_toTopOf="@+id/route_line_cicle_end_bg" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/skinTextView2"
                android:layout_width="0.5dp"
                android:layout_height="@dimen/dp_38"
                android:background="@color/route_line_color"
                app:layout_constraintBottom_toTopOf="@+id/route_item_via_endimg"
                app:layout_constraintEnd_toEndOf="@id/route_line_cicle_end_bg"
                app:layout_constraintStart_toStartOf="@id/route_line_cicle_end_bg" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/route_line_info_via_end_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxWidth="@dimen/dp_532"
                android:maxLines="1"
                android:text="@{ViewModel.getEndName()}"
                android:textColor="@color/text_route_detail_title"
                android:textSize="@dimen/sp_26"
                android:visibility="@{ViewModel.getViaPoiListVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintBottom_toBottomOf="@+id/route_item_via_endimg"
                app:layout_constraintEnd_toEndOf="@+id/route_line_info_tv_search_waypoint"
                app:layout_constraintStart_toStartOf="@id/route_line_tv_title"
                app:layout_constraintTop_toTopOf="@+id/route_item_via_endimg"
                tools:text="终点" />

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/route_line_via_open_close"
                clickCommand="@{ViewModel.getOpenCloseViaClick()}"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_54"
                android:layout_marginTop="@dimen/dp_n_20"
                android:visibility="@{ViewModel.getViaPoiListVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintTop_toBottomOf="@+id/route_line_info_via_end_name" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/bg_lines"
                android:layout_width="@dimen/dp_200"
                android:layout_height="@dimen/dp_8"
                android:layout_marginTop="@dimen/dp_2"
                android:background="@color/route_title_line_bg"
                android:visibility="@{ViewModel.getViaPoiListVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintBottom_toBottomOf="@+id/route_line_via_open_close"
                app:layout_constraintEnd_toEndOf="@+id/route_line_via_open_close"
                app:layout_constraintStart_toStartOf="@+id/route_line_via_open_close" />

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/skinView"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_10"
                android:visibility="@{ViewModel.getViaPoiListVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintTop_toBottomOf="@+id/bg_lines"
                tools:layout_editor_absoluteX="19dp" />
        </com.sgm.navi.ui.view.SkinConstraintLayout>

        <com.sgm.navi.scene.ui.route.SceneRouteSupplementPointsView
            android:id="@+id/route_Route_Supplement_Points"
            clickCommand="@{ViewModel.getSupplementClick()}"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_570"
            android:visibility="@{ViewModel.getSecondaryPoiVisibility() == 2 ? View.VISIBLE : View.GONE}"
            app:layout_constraintStart_toStartOf="parent" />


        <com.sgm.navi.scene.ui.route.SceneRouteDescendantsView
            android:id="@+id/ly_secondary_poi"
            android:layout_width="@dimen/dp_0"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_570"
            android:background="@drawable/bg_shadow_navi_ndlc_8155"
            android:clickable="true"
            android:maxWidth="@dimen/dp_1052"
            android:paddingHorizontal="@dimen/dp_29"
            android:paddingVertical="@dimen/dp_29"
            android:visibility="@{ViewModel.getSecondaryPoiVisibility() == 1 ? View.VISIBLE : View.GONE}"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.sgm.navi.ui.view.SkinView
            android:id="@+id/route_result_line"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_14"
            app:layout_constraintBottom_toBottomOf="@id/route_line_info_title"
            app:layout_constraintStart_toStartOf="parent" />

        <com.sgm.navi.ui.view.SkinConstraintLayout
            android:id="@+id/route_line_info_prefer_root"
            android:layout_width="@dimen/dp_613"
            android:layout_height="@dimen/dp_0"
            android:layout_marginTop="@dimen/dp_m_41"
            android:background="@drawable/bg_shadow_navi_ndlc_8155"
            android:clickable="true"
            android:paddingHorizontal="@dimen/dp_30"
            android:paddingVertical="@dimen/dp_34"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHeight_max="@dimen/dp_646"
            app:layout_constraintTop_toBottomOf="@+id/route_result_line"
            app:layout_constraintVertical_bias="0">

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/route_line_info_bg_prefer"
                android:layout_width="@dimen/dp_553"
                android:layout_height="@dimen/dp_78"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="19dp" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/route_line_info_tv_prefer"
                clickCommand="@{ViewModel.getPreferenceClick()}"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_26"
                android:ellipsize="end"
                android:maxWidth="@dimen/dp_530"
                android:maxLines="1"
                android:text="@{ViewModel.getPreferText()}"
                android:textColor="@color/text_route_defult"
                android:textSize="@dimen/sp_26"
                app:layout_constraintBottom_toBottomOf="@id/route_line_info_bg_prefer"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@id/route_line_info_bg_prefer"
                tools:text="高德推荐" />

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/route_line_info_img_prefer"
                clickCommand="@{ViewModel.getPreferenceClick()}"
                android:layout_width="@dimen/dp_36"
                android:layout_height="@dimen/dp_36"
                android:src="@{ViewModel.getRoutePreferenceDrawableVisibility()}"
                app:layout_constraintBottom_toBottomOf="@id/route_line_info_bg_prefer"
                app:layout_constraintStart_toEndOf="@id/route_line_info_tv_prefer"
                app:layout_constraintTop_toTopOf="@id/route_line_info_bg_prefer"
                tools:src="@drawable/img_route_down" />

            <com.sgm.navi.ui.view.SkinSwitch
                android:id="@+id/route_line_info_switch_energy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_26"
                android:background="@android:color/transparent"
                android:button="@drawable/switch_route_button"
                android:checked="false"
                android:thumb="@null"
                android:track="@null"
                android:visibility="@{ViewModel.getBatterCheckBoxVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintBottom_toBottomOf="@id/route_line_info_bg_prefer"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/route_line_info_bg_prefer" />

            <com.sgm.navi.ui.view.SkinTextView
                clickCommand="@{ViewModel.getRouteEnergyClick()}"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_10"
                android:text="@string/route_energy_replenishment_planning"
                android:textColor="@color/text_route_defult"
                android:textSize="@dimen/sp_26"
                android:visibility="@{ViewModel.getBatterCheckBoxVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintBottom_toBottomOf="@id/route_line_info_bg_prefer"
                app:layout_constraintEnd_toStartOf="@+id/route_line_info_switch_energy"
                app:layout_constraintTop_toTopOf="@id/route_line_info_bg_prefer" />

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/route_line_info_prefer_line"
                android:layout_width="@dimen/dp_553"
                android:layout_height="@dimen/dp_1"
                android:visibility="@{ViewModel.getRestrictionVisibility() ? View.GONE : View.VISIBLE}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/route_line_info_bg_prefer" />

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/route_line_info_scene_route_restriction"
                clickCommand="@{ViewModel.getRestrictionClick()}"
                android:layout_width="@dimen/dp_553"
                android:layout_height="@dimen/dp_63"
                android:background="@{ViewModel.getRestrictionBackground()}"
                android:visibility="@{ViewModel.getRestrictionVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/route_line_info_prefer_line"
                tools:background="#22FF0000" />

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/route_line_info_scene_route_restriction_bg"
                android:layout_width="@dimen/dp_40"
                android:layout_height="match_parent" />

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/route_line_info_scene_route_restriction_left_img"
                android:layout_width="@dimen/dp_65"
                android:layout_height="@dimen/dp_35"
                android:layout_marginEnd="@dimen/dp_15"
                android:src="@drawable/img_basic_car"
                android:visibility="@{ViewModel.getRestrictionCarVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintBottom_toBottomOf="@+id/route_line_info_scene_route_restriction"
                app:layout_constraintStart_toStartOf="@+id/route_line_info_tv_prefer"
                app:layout_constraintTop_toTopOf="@+id/route_line_info_scene_route_restriction" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/route_line_info_scene_route_restriction_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@{ViewModel.getRestriction()}"
                android:textColor="@{ViewModel.getRestrictionTextColor()}"
                android:textSize="@dimen/sp_22"
                android:visibility="@{ViewModel.getRestrictionVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintBottom_toBottomOf="@+id/route_line_info_scene_route_restriction"
                app:layout_constraintStart_toEndOf="@+id/route_line_info_scene_route_restriction_left_img"
                app:layout_constraintTop_toTopOf="@+id/route_line_info_scene_route_restriction"
                tools:text="设置车牌，避开限行"
                tools:textColor="#DDFF0000" />

            <com.sgm.navi.ui.view.SkinImageView
                android:layout_width="@dimen/dp_45"
                android:layout_height="@dimen/dp_45"
                android:src="@drawable/img_right_back"
                android:visibility="@{ViewModel.getRestrictionRightBackVisibility() ? View.VISIBLE : View.GONE}"
                app:layout_constraintBottom_toBottomOf="@+id/route_line_info_scene_route_restriction"
                app:layout_constraintStart_toEndOf="@+id/route_line_info_scene_route_restriction_text"
                app:layout_constraintTop_toTopOf="@+id/route_line_info_scene_route_restriction" />

            <com.sgm.navi.scene.ui.route.SceneRouteResultListView
                android:id="@+id/route_line_info_scene_route_result"
                android:layout_width="@dimen/dp_553"
                android:layout_height="0dp"
                app:layout_constraintBottom_toTopOf="@+id/skinView3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/route_line_info_scene_route_restriction" />

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/skinView3"
                android:layout_width="@dimen/dp_553"
                android:layout_height="@dimen/dp_130"
                android:background="@drawable/bg_route_result_left_right"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/route_line_info_bg_route_start_navi"
                clickCommand="@{ViewModel.getStartNaviClick()}"
                android:layout_width="@dimen/dp_293"
                android:layout_height="@dimen/dp_77"
                android:layout_marginStart="@dimen/dp_26"
                android:layout_marginBottom="@dimen/dp_25"
                android:background="@drawable/bg_route_start_navigation_button"
                app:layout_constraintEnd_toStartOf="@id/route_line_info_bg_route_refresh"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableStart="@drawable/icon_details_bottom_go_here"
                android:drawablePadding="@dimen/dp_7"
                android:gravity="center"
                android:text="@{ViewModel.getCountDownHint()}"
                android:textColor="@color/text_route_button_title"
                android:textSize="@dimen/sp_29"
                app:layout_constraintBottom_toBottomOf="@id/route_line_info_bg_route_start_navi"
                app:layout_constraintEnd_toEndOf="@id/route_line_info_bg_route_start_navi"
                app:layout_constraintStart_toStartOf="@id/route_line_info_bg_route_start_navi"
                app:layout_constraintTop_toTopOf="@id/route_line_info_bg_route_start_navi"
                tools:text="开始导航" />

            <com.sgm.navi.ui.view.SkinView
                android:id="@+id/route_line_info_bg_route_refresh"
                clickCommand="@{ViewModel.getRefreshRouteClick()}"
                android:layout_width="@dimen/dp_180"
                android:layout_height="@dimen/dp_77"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_26"
                android:layout_marginBottom="@dimen/dp_25"
                android:background="@drawable/bg_route_refresh_route_button"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/route_line_info_bg_route_start_navi" />

            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableStart="@drawable/img_route_refresh_sw1225"
                android:drawablePadding="@dimen/dp_7"
                android:gravity="center"
                android:text="@string/route_refresh"
                android:textColor="@color/text_route_defult"
                android:textSize="@dimen/sp_29"
                app:layout_constraintBottom_toBottomOf="@id/route_line_info_bg_route_refresh"
                app:layout_constraintEnd_toEndOf="@id/route_line_info_bg_route_refresh"
                app:layout_constraintStart_toStartOf="@id/route_line_info_bg_route_refresh"
