<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <import type="android.view.View"/>
        <variable
            name="viewModel"
            type="com.fy.navi.hmi.setting.others.licenses.SettingOthersLicensesViewModel" />
    </data>

    <com.fy.navi.ui.view.SkinConstraintLayout
        android:layout_width="@dimen/dp_1224"
        android:layout_height="@dimen/dp_714"
        android:clickable="true"
        android:background="@drawable/bg_setting_fragment">

        <com.fy.navi.ui.view.SkinLinearLayout
            android:id="@+id/setting_others_licenses_layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_138"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" >

            <com.fy.navi.ui.view.SkinImageView
                clickCommand="@{viewModel.mFinishLicenses}"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/dp_26"
                android:src="@drawable/img_setting_back" />

            <com.fy.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/dp_460"
                android:text="@string/setting_others_about_licenses_info"
                android:textSize="@dimen/sp_34"
                android:textColor="@color/setting_bg_tab_text_select" />
        </com.fy.navi.ui.view.SkinLinearLayout>

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/setting_others_about_licenses_icp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:textColor="@color/setting_bg_tab_text_unselect"
            android:textSize="@dimen/sp_26"
            android:text="@string/setting_others_about_licenses_icp"
            app:layout_constraintTop_toBottomOf="@+id/setting_others_licenses_layout"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/setting_others_about_licenses_company"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:textColor="@color/setting_bg_tab_text_unselect"
            android:textSize="@dimen/sp_26"
            android:text="@string/setting_others_about_licenses_company"
            app:layout_constraintTop_toBottomOf="@+id/setting_others_about_licenses_icp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/setting_others_about_licenses_qr_img"
            android:layout_width="@dimen/dp_146"
            android:layout_height="@dimen/dp_146"
            android:src="@mipmap/icon_setting_about_licenses"
            android:layout_marginTop="@dimen/dp_40"
            android:padding="@dimen/dp_7"
            android:background="@drawable/bg_we_chat"
            app:layout_constraintTop_toBottomOf="@+id/setting_others_about_licenses_company"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/setting_others_about_licenses_qr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_40"
            android:text="@string/setting_others_about_licenses_qr"
            android:textColor="@color/setting_bg_tab_text_unselect"
            android:textSize="@dimen/sp_26"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_others_about_licenses_qr_img" />

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/setting_others_about_licenses_qr_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/setting_others_about_licenses_qr_desc"
            android:textColor="@color/setting_bg_tab_text_unselect"
            android:textSize="@dimen/sp_26"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_others_about_licenses_qr"/>

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/setting_others_about_licenses_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:text="@string/setting_others_about_licenses_safe"
            android:textColor="@color/setting_bg_tab_text_unselect"
            android:textSize="@dimen/sp_26"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_others_about_licenses_qr_desc"/>
    </com.fy.navi.ui.view.SkinConstraintLayout>
</layout>