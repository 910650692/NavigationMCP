<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <import type="android.view.View" />

        <variable
            name="viewModel"
            type="com.fy.navi.hmi.search.alongway.MainAlongWaySearchViewModel" />
    </data>


    <com.fy.navi.ui.view.SkinLinearLayout
        clickCommand="@{viewModel.rootClick}"
        android:layout_width="@dimen/dp_698"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:orientation="vertical">

        <com.fy.navi.scene.ui.search.SceneMainAlongWaySearchView
            android:id="@+id/search_view"
            android:layout_width="@dimen/dp_698"
            android:layout_height="wrap_content" />

        <com.fy.navi.scene.ui.search.SceneSearchHistoryView
            android:id="@+id/scene_nested_scroll_view"
            android:layout_width="@dimen/dp_698"
            android:layout_height="@dimen/dp_490"
            android:layout_marginTop="@dimen/dp_m_38" />
    </com.fy.navi.ui.view.SkinLinearLayout>
</layout>