<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <import type="android.view.View" />

        <variable
            name="viewModel"
            type="com.sgm.navi.hmi.favorite.CollectViewModel" />
    </data>


    <com.sgm.navi.ui.view.SkinLinearLayout
        clickCommand="@{viewModel.rootClick}"
        android:layout_width="@dimen/dp_698"
        android:layout_height="match_parent"
        android:clickable="true"
        android:orientation="vertical">

        <com.sgm.navi.scene.ui.favorite.SceneCollectView
            android:id="@+id/collect_view"
            android:layout_width="@dimen/dp_698"
            android:layout_height="match_parent">

        </com.sgm.navi.scene.ui.favorite.SceneCollectView>
    </com.sgm.navi.ui.view.SkinLinearLayout>
</layout>