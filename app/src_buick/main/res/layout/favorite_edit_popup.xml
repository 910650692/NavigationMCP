<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <com.fy.navi.ui.view.SkinLinearLayout
        android:layout_width="@dimen/dp_300"
        android:layout_height="@dimen/dp_120"
        android:orientation="horizontal"
        android:background="@drawable/bg_setting_fragment">

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/favorite_item_rename"
            android:layout_width="@dimen/dp_150"
            android:layout_height="match_parent"
            android:text="@string/favorite_item_rename"
            android:textSize="@dimen/sp_28"
            android:gravity="center"
            android:textColor="@color/black"/>

        <com.fy.navi.ui.view.SkinView
            android:layout_width="@dimen/dp_1"
            android:layout_height="@dimen/dp_50"
            android:layout_gravity="center"
            android:background="@color/setting_tab_gray"/>

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/favorite_item_delete"
            android:layout_width="@dimen/dp_150"
            android:layout_height="match_parent"
            android:text="@string/favorite_item_delete"
            android:textSize="@dimen/sp_28"
            android:gravity="center"
            android:textColor="@color/black"/>

    </com.fy.navi.ui.view.SkinLinearLayout>

</layout>
