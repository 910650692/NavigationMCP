<?xml version="1.0" encoding="utf-8"?>
<com.fy.navi.ui.view.SkinConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_140"
    android:layout_marginBottom="@dimen/dp_24"
    android:background="@drawable/bg_setting_fragment">

    <com.fy.navi.ui.view.SkinTextView
        android:id="@+id/item_province_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/offline_item_province_title"
        android:textColor="@color/offline_black"
        android:textSize="@dimen/sp_34"
        android:layout_marginStart="@dimen/dp_30"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

     <com.fy.navi.ui.view.SkinTextView
         android:id="@+id/item_province_data"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginStart="@dimen/dp_20"
         app:layout_constraintBottom_toBottomOf="parent"
         app:layout_constraintTop_toTopOf="parent"
         app:layout_constraintLeft_toRightOf="@+id/item_province_name"
         android:gravity="center"
         android:textColor="@color/setting_text_gray"
         android:textSize="@dimen/sp_32" />

    <com.fy.navi.ui.view.SkinImageView
        android:id="@+id/item_province_expand"
        android:layout_width="@dimen/dp_60"
        android:layout_height="@dimen/dp_60"
        android:layout_marginEnd="@dimen/dp_40"
        android:src="@drawable/img_under_the_58"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</com.fy.navi.ui.view.SkinConstraintLayout>