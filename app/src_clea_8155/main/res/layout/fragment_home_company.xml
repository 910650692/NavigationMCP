<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <import type="android.view.View" />

        <variable
            name="viewModel"
            type="com.sgm.navi.hmi.favorite.HomeCompanyViewModel" />
    </data>


    <com.sgm.navi.ui.view.SkinLinearLayout
        clickCommand="@{viewModel.mRootClick}"
        android:layout_width="@dimen/dp_545"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/dp_34"
        android:layout_marginTop="@dimen/dp_34"
        android:clickable="true"
        android:orientation="vertical">

        <com.sgm.navi.scene.ui.favorite.SceneHomeCompanyView
            android:id="@+id/home_company_view"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_223" />

        <com.sgm.navi.scene.ui.search.SceneSearchHistoryView
            android:id="@+id/scene_nested_scroll_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/dp_17"
            android:layout_marginBottom="@dimen/dp_35" />
    </com.sgm.navi.ui.view.SkinLinearLayout>
</layout>