<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.sgm.navi.ui.view.SkinConstraintLayout
        android:id="@+id/item_manager_province_view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_140"
        android:clickable="true">

        <com.sgm.navi.ui.view.SkinTextView
            android:id="@+id/item_manager_province_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_20"
            android:text="@string/offline_item_city_title"
            android:textColor="@color/setting_bg_tab_text_select"
            android:textSize="@dimen/sp_34"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </com.sgm.navi.ui.view.SkinConstraintLayout>

</layout>