<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <import type="android.view.View"/>
        <variable
            name="viewModel"
            type="com.sgm.navi.hmi.setting.others.about.SettingOthersAboutViewModel" />
    </data>

    <com.sgm.navi.ui.view.SkinConstraintLayout
        android:layout_width="@dimen/dp_1680"
        android:layout_height="@dimen/dp_1102"
        android:layout_marginStart="@dimen/dp_259"
        android:focusable="false"
        android:padding="@dimen/dp_51"
        android:background="@drawable/bg_shadow_navi_cadillac">

        <com.android.car.ui.FocusParkingView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.sgm.navi.ui.view.SkinLinearLayout
            android:id="@+id/setting_others_about_layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_172"
            android:orientation="horizontal"
            android:focusable="false"
            android:gravity="center_vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" >

            <com.android.car.ui.FocusArea
                android:id="@+id/focus_close"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_32"
                app:nudgeDown="@+id/setting_others_about_focus"
                app:nudgeUp="@+id/setting_others_about_focus">

                <com.sgm.navi.ui.view.SkinImageView
		    android:contentDescription="@string/app_currency_back"
                    style="@style/style_mfc_config"
                    clickCommand="@{viewModel.mFinishAbout}"
                    android:layout_width="@dimen/dp_74"
                    android:layout_height="@dimen/dp_74"
                    android:src="@drawable/img_setting_back" />
            </com.android.car.ui.FocusArea>



            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_522"
                android:text="@string/setting_others_about_title"
                android:textSize="@dimen/sp_46"
                android:focusable="false"
                android:textColor="@color/setting_bg_tab_text_select" />
        </com.sgm.navi.ui.view.SkinLinearLayout>

        <com.sgm.navi.ui.view.SkinLinearLayout
            android:id="@+id/setting_others_about_image_layout"
            android:layout_width="@dimen/dp_600"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:focusable="false"
            android:layout_marginTop="@dimen/dp_16"
            android:layout_marginStart="@dimen/dp_123"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_others_about_layout">

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/setting_others_about_image"
                android:layout_width="@dimen/dp_374"
                android:layout_height="@dimen/dp_275"
                android:focusable="false"
                android:src="@mipmap/icon_setting_about" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/setting_others_about_version"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@{viewModel.getVersionName()}"
                android:textColor="@color/setting_bg_tab_text_unselect"
                android:textSize="@dimen/sp_38"
                android:layout_marginTop="@dimen/dp_32"
                android:focusable="false"
                app:layout_constraintTop_toBottomOf="@+id/setting_others_about_image"
                app:layout_constraintStart_toStartOf="@+id/setting_others_about_image"/>
        </com.sgm.navi.ui.view.SkinLinearLayout>


        <com.sgm.navi.ui.view.SkinLinearLayout
            android:layout_width="@dimen/dp_576"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:focusable="false"
            android:layout_marginStart="@dimen/dp_52"
            android:layout_marginTop="@dimen/dp_167"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_others_about_image_layout">

            <com.sgm.navi.ui.view.SkinLinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:focusable="false">

                <com.sgm.navi.ui.view.SkinTextView
                    android:id="@+id/setting_others_about_channel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/setting_others_about_channel"
                    android:focusable="false"
                    android:textColor="@color/setting_bg_tab_text_unselect"
                    android:textSize="@dimen/sp_32" />

                <com.sgm.navi.ui.view.SkinTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{viewModel.mChannelID}"
                    android:textColor="@color/setting_bg_tab_text_unselect"
                    android:textSize="@dimen/sp_32"
                    android:focusable="false"
                    android:layout_marginStart="@dimen/dp_10"/>
            </com.sgm.navi.ui.view.SkinLinearLayout>

           <com.sgm.navi.ui.view.SkinLinearLayout
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               android:focusable="false"
               android:layout_marginTop="@dimen/dp_16">

               <com.sgm.navi.ui.view.SkinTextView
                   android:id="@+id/setting_others_about_data_version"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="@string/setting_others_about_data_version"
                   android:focusable="false"
                   android:textColor="@color/setting_bg_tab_text_unselect"
                   android:textSize="@dimen/sp_32" />

               <com.sgm.navi.ui.view.SkinTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="@{viewModel.mMapDataVersion}"
                   android:textColor="@color/setting_bg_tab_text_unselect"
                   android:textSize="@dimen/sp_32"
                   android:focusable="false"
                   android:layout_marginStart="@dimen/dp_10" />

           </com.sgm.navi.ui.view.SkinLinearLayout>

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/setting_others_about_draw"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@{viewModel.mVersion}"
                android:textColor="@color/setting_bg_tab_text_unselect"
                android:textSize="@dimen/sp_32"
                android:layout_marginTop="@dimen/dp_16"
                android:focusable="false"
                app:layout_constraintStart_toStartOf="@+id/setting_others_about_data_version"
                app:layout_constraintTop_toBottomOf="@+id/setting_others_about_data_version"/>
        </com.sgm.navi.ui.view.SkinLinearLayout>

        <com.sgm.navi.ui.view.SkinView
            android:id="@+id/line_view_vertical"
            android:layout_width="@dimen/dp_1"
            android:layout_height="@dimen/dp_920"
            android:layout_marginStart="@dimen/dp_820"
            android:focusable="false"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_others_about_layout" />

        <com.android.car.ui.FocusArea
            android:id="@+id/setting_others_about_focus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:nudgeDown="@+id/focus_close"
            app:nudgeUp="@+id/focus_close"
            app:layout_constraintStart_toEndOf="@id/line_view_vertical"
            app:layout_constraintTop_toBottomOf="@id/setting_others_about_layout">

            <com.sgm.navi.ui.view.SkinConstraintLayout
                clickCommand="@{viewModel.mOpenProtocolCenter}"
                style="@style/style_mfc_config_no_bg"
                android:background="@drawable/bg_setting_about_mfc_selector"
                android:id="@+id/setting_others_about_protocol_center"
                android:layout_width="@dimen/dp_706"
                android:layout_height="@dimen/dp_144"
                app:layout_constraintStart_toEndOf="@id/line_view_vertical"
                app:layout_constraintTop_toBottomOf="@id/setting_others_about_layout">

                <com.sgm.navi.ui.view.SkinTextView
                    android:id="@+id/setting_others_about_protocol_center_text"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/setting_others_privacy_protocol_center"
                    android:layout_marginStart="@dimen/dp_44"
                    android:textSize="@dimen/sp_46"
                    android:textColor="@color/setting_bg_tab_text_select"
                    android:gravity="center"
                    android:focusable="false"
                    app:layout_constraintStart_toStartOf="parent"/>

                <com.sgm.navi.ui.view.SkinImageView
                    android:layout_width="@dimen/dp_74"
                    android:layout_height="@dimen/dp_74"
                    android:src="@drawable/img_setting_corner"
                    android:gravity="center"
                    android:focusable="false"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:layout_marginBottom="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>

            </com.sgm.navi.ui.view.SkinConstraintLayout>
            <com.sgm.navi.ui.view.SkinConstraintLayout
                clickCommand="@{viewModel.mOpenLicensesInfo}"
                style="@style/style_mfc_config_no_bg"
                android:background="@drawable/bg_setting_about_mfc_selector"
                android:id="@+id/setting_others_about_licenses_info"
                android:layout_width="@dimen/dp_706"
                android:layout_height="@dimen/dp_144"
                android:layout_marginTop="@dimen/dp_16"
                app:layout_constraintStart_toEndOf="@id/line_view_vertical"
                app:layout_constraintTop_toBottomOf="@id/setting_others_about_protocol_center">

                <com.sgm.navi.ui.view.SkinTextView
                    android:id="@+id/setting_others_about_licenses_info_title"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/setting_others_about_licenses_info"
                    android:layout_marginStart="@dimen/dp_44"
                    android:textSize="@dimen/sp_46"
                    android:textColor="@color/setting_bg_tab_text_select"
                    android:gravity="center"
                    android:focusable="false"
                    app:layout_constraintStart_toStartOf="parent"/>

                <com.sgm.navi.ui.view.SkinImageView
                    android:layout_width="@dimen/dp_74"
                    android:layout_height="@dimen/dp_74"
                    android:src="@drawable/img_setting_corner"
                    android:gravity="center"
                    android:focusable="false"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:layout_marginBottom="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>

            </com.sgm.navi.ui.view.SkinConstraintLayout>
            <com.sgm.navi.ui.view.SkinConstraintLayout
                clickCommand="@{viewModel.mOpenHelp}"
                style="@style/style_mfc_config_no_bg"
                android:background="@drawable/bg_setting_about_mfc_selector"
                android:id="@+id/setting_others_about_help"
                android:layout_width="@dimen/dp_706"
                android:layout_height="@dimen/dp_144"
                android:layout_marginTop="@dimen/dp_16"
                android:visibility="gone"
                app:layout_constraintStart_toEndOf="@id/line_view_vertical"
                app:layout_constraintTop_toBottomOf="@id/setting_others_about_licenses_info">

                <com.sgm.navi.ui.view.SkinTextView
                    android:id="@+id/setting_others_about_help_title"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/setting_others_about_help"
                    android:layout_marginStart="@dimen/dp_44"
                    android:textSize="@dimen/sp_46"
                    android:textColor="@color/setting_bg_tab_text_select"
                    android:gravity="center"
                    android:focusable="false"
                    app:layout_constraintStart_toStartOf="parent"/>

                <com.sgm.navi.ui.view.SkinImageView
                    android:layout_width="@dimen/dp_74"
                    android:layout_height="@dimen/dp_74"
                    android:src="@drawable/img_setting_corner"
                    android:gravity="center"
                    android:focusable="false"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:layout_marginBottom="@dimen/dp_10"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>

            </com.sgm.navi.ui.view.SkinConstraintLayout>

        </com.android.car.ui.FocusArea>

        <com.sgm.navi.ui.view.SkinTextView
            android:id="@+id/setting_others_about_contact_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/setting_others_about_contact"
            android:layout_marginTop="@dimen/dp_24"
            android:textSize="@dimen/sp_32"
            android:focusable="false"
            android:textColor="@color/setting_bg_tab_text_unselect"
            app:layout_constraintStart_toEndOf="@id/line_view_vertical"
            app:layout_constraintTop_toBottomOf="@id/setting_others_about_focus"/>

        <com.sgm.navi.ui.view.SkinLinearLayout
            android:id="@+id/setting_others_about_web"
            android:layout_width="@dimen/dp_350"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:focusable="false"
            android:layout_marginTop="@dimen/dp_34"
            app:layout_constraintStart_toEndOf="@id/line_view_vertical"
            app:layout_constraintTop_toBottomOf="@id/setting_others_about_contact_title">

            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_others_about_web"
                android:textSize="@dimen/sp_38"
                android:focusable="false"
                android:textColor="@color/setting_car_logo_text_gray" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/setting_others_about_url"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_others_about_url"
                android:textSize="@dimen/sp_32"
                android:focusable="false"
                android:textColor="@color/setting_car_logo_text_gray"
                app:layout_constraintStart_toEndOf="@id/line_view_vertical"
                app:layout_constraintTop_toBottomOf="@id/setting_others_about_web"/>

        </com.sgm.navi.ui.view.SkinLinearLayout>

        <com.sgm.navi.ui.view.SkinView
            android:layout_width="@dimen/dp_706"
            android:layout_height="@dimen/dp_1"
            android:layout_marginTop="@dimen/dp_16"
            android:focusable="false"
            app:layout_constraintStart_toStartOf="@id/setting_others_about_web"
            app:layout_constraintTop_toBottomOf="@+id/setting_others_about_web"
            android:background="@color/setting_bg_line"/>

        <com.sgm.navi.ui.view.SkinView
            android:layout_width="@dimen/dp_1"
            android:layout_height="@dimen/dp_90"
            android:focusable="false"
            app:layout_constraintStart_toEndOf="@id/setting_others_about_web"
            app:layout_constraintTop_toTopOf="@+id/setting_others_about_web"
            android:background="@color/setting_bg_line"/>

        <com.sgm.navi.ui.view.SkinLinearLayout
            android:id="@+id/setting_others_about_wechat"
            android:layout_width="@dimen/dp_350"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:focusable="false"
            android:layout_marginTop="@dimen/dp_34"
            app:layout_constraintStart_toStartOf="@id/line_view_vertical"
            app:layout_constraintTop_toBottomOf="@id/setting_others_about_web">

            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_others_about_wechat"
                android:textSize="@dimen/sp_38"
                android:focusable="false"
                android:textColor="@color/setting_car_logo_text_gray" />

            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_others_about_title"
                android:textSize="@dimen/sp_32"
                android:focusable="false"
                android:textColor="@color/setting_car_logo_text_gray" />

        </com.sgm.navi.ui.view.SkinLinearLayout>

        <com.sgm.navi.ui.view.SkinView
            android:layout_width="@dimen/dp_1"
            android:layout_height="@dimen/dp_90"
            android:focusable="false"
            app:layout_constraintStart_toEndOf="@id/setting_others_about_wechat"
            app:layout_constraintTop_toTopOf="@+id/setting_others_about_wechat"
            android:background="@color/setting_bg_line"/>

        <com.sgm.navi.ui.view.SkinLinearLayout
            android:id="@+id/setting_others_about_tel"
            android:layout_width="@dimen/dp_350"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:focusable="false"
            app:layout_constraintStart_toEndOf="@id/setting_others_about_web"
            app:layout_constraintTop_toTopOf="@id/setting_others_about_web">

            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_others_about_tel"
                android:textSize="@dimen/sp_38"
                android:focusable="false"
                android:textColor="@color/setting_car_logo_text_gray" />

            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_others_about_tel_number"
                android:textSize="@dimen/sp_32"
                android:focusable="false"
                android:textColor="@color/setting_car_logo_text_gray" />

        </com.sgm.navi.ui.view.SkinLinearLayout>

        <com.sgm.navi.ui.view.SkinLinearLayout
            android:id="@+id/setting_others_about_qq"
            android:layout_width="@dimen/dp_350"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:focusable="false"
            app:layout_constraintStart_toStartOf="@+id/setting_others_about_tel"
            app:layout_constraintTop_toTopOf="@id/setting_others_about_wechat">

            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_others_about_qq"
                android:textSize="@dimen/sp_38"
                android:focusable="false"
                android:textColor="@color/setting_car_logo_text_gray" />

            <com.sgm.navi.ui.view.SkinTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_others_about_qq_number"
                android:textSize="@dimen/sp_32"
                android:focusable="false"
                android:textColor="@color/setting_car_logo_text_gray" />

        </com.sgm.navi.ui.view.SkinLinearLayout>

    </com.sgm.navi.ui.view.SkinConstraintLayout>
</layout>
