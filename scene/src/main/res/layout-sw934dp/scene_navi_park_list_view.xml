<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />

        <variable
            name="naviParkList"
            type="com.fy.navi.scene.api.navi.ISceneNaviParkList" />
    </data>

    <com.fy.navi.ui.view.SkinConstraintLayout
        android:id="@+id/scl_park_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        tools:background="@color/navi_red_text"
        tools:layout_height="@dimen/dp_918"
        tools:layout_width="@dimen/dp_820">

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/siv_close_park"
            clickCommand="@{naviParkList.closeParkList}"
            android:layout_width="@dimen/dp_68"
            android:layout_height="@dimen/dp_68"
            android:layout_marginTop="@dimen/dp_24"
            android:layout_marginEnd="@dimen/dp_30"
            android:background="@drawable/img_navi_close"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fillViewport="true"
            app:layout_constraintTop_toBottomOf="@id/siv_close_park">

            <com.fy.navi.ui.view.SkinRecyclerView
                android:id="@+id/srv_add_via"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/dp_40"
                android:layout_marginTop="@dimen/dp_16" />
        </androidx.core.widget.NestedScrollView>
    </com.fy.navi.ui.view.SkinConstraintLayout>
</layout>





