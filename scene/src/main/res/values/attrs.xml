<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SceneSearchPoiList">
        <attr name="layoutType" />
    </declare-styleable>

    <declare-styleable name="SceneSearchBarView">
        <attr name="layoutType" />
    </declare-styleable>

    <declare-styleable name="CommonLayoutType">
        <attr name="layoutType" format="enum">
            <enum name="scene_sug_search_fragment" value="0" />
            <enum name="scene_search_result_fragment" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SceneVerticalTabLayout">
        <attr name="isMarginBottom" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ProvinceKeyboardView">
        <attr name="isDefault" format="boolean" />
        <attr name="rowWidth" format="float" />
        <attr name="rowHeight" format="float" />
        <attr name="provinceTextSize" format="float" />
    </declare-styleable>

    <declare-styleable name="PlateNumberKeyboardView">
        <attr name="isDefaultCar" format="boolean" />
        <attr name="numberRowHeight" format="float" />
        <attr name="textSize" format="float" />
        <attr name="numberRowWidth" format="float" />
        <attr name="thirdRowWidth" format="float" />
        <attr name="lastRowWidth" format="float" />
        <attr name="insertWidth" format="float" />
    </declare-styleable>
</resources>