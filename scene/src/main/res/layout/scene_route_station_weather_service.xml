<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />

        <variable
            name="Scene"
            type="com.fy.navi.scene.api.route.ISceneRouteGasStationWeatherService" />
    </data>

    <com.fy.navi.ui.view.SkinConstraintLayout
        android:layout_width="@dimen/dp_146"
        android:layout_height="@dimen/dp_436">

        <com.fy.navi.ui.view.SkinView
            android:id="@+id/route_right_tab_list_service"
            clickCommand="@{Scene.clickService}"
            android:layout_width="@dimen/dp_146"
            android:layout_height="@dimen/dp_146"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/route_right_tab_list_tv_service"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/route_along_service"
            android:textSize="@dimen/sp_30"
            android:textColor="@color/tab_list_route_tab_list_unselect"
            android:layout_marginBottom="@dimen/dp_28"
            app:select_text_color="@color/tab_list_route_tab_list_select"
            app:layout_constraintEnd_toEndOf="@id/route_right_tab_list_service"
            app:layout_constraintStart_toStartOf="@id/route_right_tab_list_service"
            app:layout_constraintBottom_toBottomOf="@id/route_right_tab_list_service"/>

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/route_right_tab_list_iv_service"
            android:layout_width="@dimen/dp_58"
            android:layout_height="@dimen/dp_58"
            android:src="@drawable/selector_route_tab_list_service"
            android:layout_marginBottom="@dimen/dp_4"
            app:layout_constraintEnd_toEndOf="@id/route_right_tab_list_service"
            app:layout_constraintStart_toStartOf="@id/route_right_tab_list_service"
            app:layout_constraintBottom_toTopOf="@id/route_right_tab_list_tv_service"/>

        <com.fy.navi.ui.view.SkinView
            android:id="@+id/route_right_tab_list_weather"
            clickCommand="@{Scene.clickWeather}"
            android:layout_width="@dimen/dp_146"
            android:layout_height="@dimen/dp_146"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/route_right_tab_list_service"
            />

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/route_right_tab_list_tv_weather"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/route_weather"
            android:textColor="@color/tab_list_route_tab_list_unselect"
            android:textSize="@dimen/sp_30"
            android:layout_marginBottom="@dimen/dp_28"
            app:select_text_color="@color/tab_list_route_tab_list_select"
            app:layout_constraintEnd_toEndOf="@id/route_right_tab_list_weather"
            app:layout_constraintStart_toStartOf="@id/route_right_tab_list_weather"
            app:layout_constraintBottom_toBottomOf="@id/route_right_tab_list_weather"/>

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/route_right_tab_list_iv_weather"
            android:layout_width="@dimen/dp_58"
            android:layout_height="@dimen/dp_58"
            android:src="@drawable/selector_route_tab_list_weather"
            android:layout_marginBottom="@dimen/dp_4"
            app:layout_constraintEnd_toEndOf="@id/route_right_tab_list_weather"
            app:layout_constraintStart_toStartOf="@id/route_right_tab_list_weather"
            app:layout_constraintBottom_toTopOf="@id/route_right_tab_list_tv_weather"
            />

        <com.fy.navi.ui.view.SkinView
            clickCommand="@{Scene.clickGasStation}"
            android:id="@+id/route_right_tab_list_gas_station"
            android:layout_width="@dimen/dp_146"
            android:layout_height="@dimen/dp_146"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/route_right_tab_list_weather"
            />

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/route_right_tab_list_tv_gas_station"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/route_gas_station"
            android:textColor="@color/tab_list_route_tab_list_unselect"
            android:textSize="@dimen/sp_30"
            android:layout_marginBottom="@dimen/dp_28"
            app:select_text_color="@color/tab_list_route_tab_list_select"
            app:layout_constraintEnd_toEndOf="@id/route_right_tab_list_gas_station"
            app:layout_constraintStart_toStartOf="@id/route_right_tab_list_gas_station"
            app:layout_constraintBottom_toBottomOf="@id/route_right_tab_list_gas_station"/>

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/route_right_tab_list_iv_gas_station"
            android:layout_width="@dimen/dp_58"
            android:layout_height="@dimen/dp_58"
            android:src="@drawable/selector_route_tab_list_charge_station"
            android:layout_marginBottom="@dimen/dp_4"
            app:layout_constraintEnd_toEndOf="@id/route_right_tab_list_gas_station"
            app:layout_constraintStart_toStartOf="@id/route_right_tab_list_gas_station"
            app:layout_constraintBottom_toTopOf="@id/route_right_tab_list_tv_gas_station"/>
    </com.fy.navi.ui.view.SkinConstraintLayout>

</layout>