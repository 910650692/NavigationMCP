<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="MissingDefaultResource">

    <com.fy.navi.ui.view.SkinConstraintLayout
        android:layout_width="@dimen/dp_411"
        android:clickable="true"
        android:layout_height="@dimen/dp_157"
        android:background="@drawable/bg_search_main_tab"
        android:paddingStart="@dimen/dp_26"
        android:paddingTop="@dimen/dp_26"
        android:paddingEnd="@dimen/dp_22">

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/textViewTimeRemaining"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_32"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/textViewEstimatedArrival"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_14"
            android:textSize="@dimen/sp_26"
            android:textColor="@color/search_color_ffffff_70"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textViewTimeRemaining" />

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/buttonGoHome"
            android:layout_width="@dimen/dp_111"
            android:layout_height="@dimen/dp_65"
            android:gravity="center"
            android:layout_marginTop="@dimen/dp_13"
            android:background="@drawable/bg_navi_go_charge"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_26"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

        <com.fy.navi.scene.ui.map.component.MultiStageProgressBar
            android:id="@+id/sk_iv_basic_home_progress"
            android:layout_width="@dimen/dp_361"
            android:layout_height="@dimen/dp_3"
            android:layout_marginBottom="@dimen/dp_28"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="@dimen/dp_13"
            app:layout_constraintTop_toBottomOf="@+id/textViewEstimatedArrival" />

    </com.fy.navi.ui.view.SkinConstraintLayout>
</layout>