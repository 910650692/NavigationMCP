<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>
    <com.sgm.navi.ui.view.SkinConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <com.sgm.navi.ui.view.SkinConstraintLayout
        android:background="@drawable/bg_shadow_system_cadillac"
        android:layout_width="@dimen/dp_1600"
        android:paddingHorizontal="@dimen/dp_47"
        android:paddingTop="@dimen/dp_39"
        android:paddingBottom="@dimen/dp_56"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_height="wrap_content">
        <com.sgm.navi.ui.view.SkinTextView
            android:id="@+id/lp_title"
            tools:text="点击拨打以下电话:"
            android:textSize="@dimen/sp_32"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="@dimen/dp_56"
            android:textColor="@color/search_color_ffffff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <com.sgm.navi.ui.view.SkinRecyclerView
            android:id="@+id/lp_phone_list"
            app:layout_constraintTop_toBottomOf="@+id/lp_title"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/lp_confim"
            android:layout_marginTop="@dimen/dp_27"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <com.sgm.navi.ui.view.SkinTextView
            android:id="@+id/lp_confim"
            app:layout_constraintTop_toBottomOf="@+id/lp_phone_list"
            android:background="@drawable/clear_dialog_cancel_bg"
            android:layout_width="@dimen/dp_475"
            android:layout_height="@dimen/dp_80"
            android:layout_marginTop="@dimen/dp_55"
            android:text="@string/dsc_cancel"
            android:textColor="@color/search_color_ffffff"
            android:textSize="@dimen/sp_29"
            android:gravity="center"
            android:layout_marginBottom="@dimen/dp_48"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    </com.sgm.navi.ui.view.SkinConstraintLayout>
    </com.sgm.navi.ui.view.SkinConstraintLayout>
</layout>