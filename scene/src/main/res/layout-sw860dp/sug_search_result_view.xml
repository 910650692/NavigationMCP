<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="ISceneSugSearchPoiList"
            type="com.fy.navi.scene.api.search.ISceneSearchPoiList" />
    </data>

    <com.fy.navi.ui.view.SkinLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/scl_search_top_view"
            layout="@layout/search_edit_bar_view"
            android:layout_width="@dimen/dp_820"
            android:layout_height="@dimen/dp_102" />

        <com.fy.navi.ui.view.SkinConstraintLayout
            android:layout_width="@dimen/dp_820"
            android:layout_height="@dimen/dp_610"
            android:layout_marginTop="@dimen/dp_24"
            android:background="@drawable/bg_search_main_tab">

            <com.fy.navi.ui.view.SkinRecyclerView
                android:id="@+id/recycler_search_result"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </com.fy.navi.ui.view.SkinConstraintLayout>

    </com.fy.navi.ui.view.SkinLinearLayout>

</layout>