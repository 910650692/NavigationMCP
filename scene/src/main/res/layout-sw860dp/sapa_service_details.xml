<?xml version="1.0" encoding="utf-8"?>
<com.fy.navi.ui.view.SkinConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dp_640"
    android:layout_height="@dimen/dp_711"
    android:background="@drawable/bg_navi_control"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.fy.navi.ui.view.SkinTextView
        android:id="@+id/siv_service_details_status_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_poi_gas_price_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_42"
        android:textSize="@dimen/sp_26"
        android:paddingHorizontal="@dimen/dp_12"
        android:paddingVertical="@dimen/dp_6"
        android:gravity="center"
        android:textColor="@color/color_000000_50"
        tools:text="维护中" />

    <com.fy.navi.ui.view.SkinTextView
        android:id="@+id/siv_service_details_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/siv_service_details_status_tag"
        app:layout_constraintBottom_toBottomOf="@+id/siv_service_details_status_tag"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="@dimen/dp_158"
        android:textSize="@dimen/sp_38"
        android:textColor="@color/color_000000_100"
        tools:text="商城服务区"/>

    <com.fy.navi.ui.view.SkinTextView
        android:id="@+id/siv_service_details_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_120"
        android:textSize="@dimen/sp_34"
        android:textColor="@color/color_000000_100"
        tools:text="5.2公里"/>

    <com.fy.navi.ui.view.SkinTextView
        android:id="@+id/siv_service_details_remaintime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@+id/siv_service_details_distance"
        app:layout_constraintTop_toTopOf="@+id/siv_service_details_distance"
        app:layout_constraintBottom_toBottomOf="@+id/siv_service_details_distance"
        android:layout_marginStart="@dimen/dp_38"
        android:textSize="@dimen/sp_34"
        android:textColor="@color/color_000000_100"
        tools:text="30分钟"/>

    <com.fy.navi.ui.view.SkinTextView
        android:id="@+id/siv_service_details_remiancharge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_120"
        android:textSize="@dimen/sp_34"
        android:textColor="@color/color_000000_100"
        tools:text="15%到达"/>

    <com.fy.navi.ui.view.SkinImageView
        android:id="@+id/siv_service_remain_charge_icon"
        android:layout_width="@dimen/dp_37"
        android:layout_height="@dimen/dp_37"
        android:background="@drawable/img_electricity_medium_42"
        app:layout_constraintTop_toTopOf="@+id/siv_service_details_remiancharge"
        app:layout_constraintBottom_toBottomOf="@+id/siv_service_details_remiancharge"
        app:layout_constraintEnd_toStartOf="@+id/siv_service_details_remiancharge"
        android:layout_marginEnd="@dimen/dp_7" />

    <com.fy.navi.ui.view.SkinImageView
        android:id="@+id/siv_service_exit"
        android:layout_width="@dimen/dp_50"
        android:layout_height="@dimen/dp_50"
        android:background="@drawable/img_off58"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/dp_41"
        android:layout_marginEnd="@dimen/dp_30"/>

    <com.fy.navi.ui.view.SkinTextView
        android:id="@+id/siv_service_details_route_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_181"
        android:textSize="@dimen/sp_30"
        android:textColor="@color/color_000000_70"
        tools:text="浦东新区世纪大道1号楼（进华经北路)"/>

    <com.fy.navi.ui.view.SkinConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_52"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/sapa_service_details_icon_bg"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_250">

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/siv_service_details_service_0"
            android:layout_width="@dimen/dp_42"
            android:layout_height="@dimen/dp_42"
            android:layout_marginStart="@dimen/dp_11"
            android:background="@drawable/img_lightning_black_42"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/siv_service_details_service_1"
            android:layout_width="@dimen/dp_42"
            android:layout_height="@dimen/dp_42"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/siv_service_details_service_0"
            android:layout_marginStart="@dimen/dp_18"
            android:background="@drawable/img_gourmet_black_42"/>

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/siv_service_details_service_2"
            android:layout_width="@dimen/dp_42"
            android:layout_height="@dimen/dp_42"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/siv_service_details_service_1"
            android:layout_marginStart="@dimen/dp_18"
            android:background="@drawable/img_maintenance_black_42"/>

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/siv_service_details_service_3"
            android:layout_width="@dimen/dp_42"
            android:layout_height="@dimen/dp_42"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/siv_service_details_service_2"
            android:layout_marginStart="@dimen/dp_18"
            android:background="@drawable/img_lavatory_black_42"/>

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/siv_service_details_service_4"
            android:layout_width="@dimen/dp_42"
            android:layout_height="@dimen/dp_42"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/siv_service_details_service_3"
            android:layout_marginStart="@dimen/dp_18"
            android:background="@drawable/img_buy_black_42"/>

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/siv_service_details_service_5"
            android:layout_width="@dimen/dp_42"
            android:layout_height="@dimen/dp_42"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/siv_service_details_service_4"
            android:layout_marginStart="@dimen/dp_18"
            android:background="@drawable/img_groggery_black_42"/>


    </com.fy.navi.ui.view.SkinConstraintLayout>

    <com.fy.navi.ui.view.SkinConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_52"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/sapa_service_details_icon_bg"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_326">

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/siv_service_details_gas_icon_bg"
            android:layout_width="@dimen/dp_52"
            android:layout_height="@dimen/dp_52"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:background="@color/color_B2AFC2_15"/>

        <com.fy.navi.ui.view.SkinImageView
            android:id="@+id/siv_service_details_gas_icon"
            android:layout_width="@dimen/dp_36"
            android:layout_height="@dimen/dp_36"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="@dimen/dp_8" />

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/siv_service_details_gas_type_0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/siv_service_details_gas_icon"
            android:layout_marginStart="@dimen/dp_26"
            android:textColor="@color/color_000000_70"
            android:textSize="@dimen/sp_30"
            tools:text="98#"/>

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/siv_service_details_gas_type_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/siv_service_details_gas_type_0"
            android:layout_marginStart="@dimen/dp_27"
            android:textColor="@color/color_000000_70"
            android:textSize="@dimen/sp_30"
            tools:text="95#"/>

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/siv_service_details_gas_type_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/siv_service_details_gas_type_1"
            android:layout_marginStart="@dimen/dp_26"
            android:textColor="@color/color_000000_70"
            android:textSize="@dimen/sp_30"
            tools:text="92#"/>

        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/siv_service_details_gas_type_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/siv_service_details_gas_type_2"
            android:layout_marginStart="@dimen/dp_26"
            android:textColor="@color/color_000000_70"
            android:textSize="@dimen/sp_30"
            android:paddingEnd="@dimen/dp_18"
            tools:text="0#"/>

    </com.fy.navi.ui.view.SkinConstraintLayout>

    <com.fy.navi.ui.view.SkinImageView
        android:id="@+id/siv_horizontal_line"
        android:layout_width="@dimen/dp_560"
        android:layout_height="@dimen/dp_1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@color/color_000000_40"
        android:layout_marginTop="@dimen/dp_408"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.fy.navi.ui.view.SkinTextView
        android:id="@+id/siv_service_details_business_hours"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/siv_horizontal_line"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_30"
        android:textSize="@dimen/sp_30"
        android:textColor="@color/color_000000_70"
        tools:text="营业时间：00:00—24:00"/>

    <com.fy.navi.ui.view.SkinTextView
        android:id="@+id/siv_service_details_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/siv_horizontal_line"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_94"
        android:textSize="@dimen/sp_30"
        android:textColor="@color/color_000000_70"
        tools:text="电        话：00:00—24:00"/>

    <com.fy.navi.scene.ui.navi.view.ButtonText
        android:id="@+id/bt_service_details_add_via"
        android:layout_width="@dimen/dp_470"
        android:layout_height="@dimen/dp_90"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/sapa_add_via_bg"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginBottom="@dimen/dp_40"
        android:textSize="@dimen/sp_34"
        android:gravity="center"
        android:text="@string/route_service_details_add_via"
        android:textColor="@color/navi_color_FFFFFF_100"/>

    <com.fy.navi.ui.view.SkinConstraintLayout
        android:id="@+id/scl_service_details_tel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="@dimen/dp_40"
        android:layout_marginBottom="@dimen/dp_38">

        <com.fy.navi.ui.view.SkinImageView
            android:layout_width="@dimen/dp_58"
            android:layout_height="@dimen/dp_58"
            android:background="@drawable/img_telephone_58"
            app:layout_constraintBottom_toTopOf="@+id/stv_service_details_tel_text"
            android:layout_marginTop="@dimen/dp_4"
            app:layout_constraintStart_toStartOf="@+id/stv_service_details_tel_text"
            app:layout_constraintEnd_toEndOf="@+id/stv_service_details_tel_text"/>


        <com.fy.navi.ui.view.SkinTextView
            android:id="@+id/stv_service_details_tel_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:textSize="@dimen/sp_30"
            android:textColor="@color/color_000000_70"
            android:text="@string/navi_tel"/>


    </com.fy.navi.ui.view.SkinConstraintLayout>
    


</com.fy.navi.ui.view.SkinConstraintLayout>