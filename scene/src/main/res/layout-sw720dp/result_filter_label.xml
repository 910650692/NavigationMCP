<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <import type="android.view.View" />
    </data>

    <com.sgm.navi.ui.view.SkinConstraintLayout
        android:id="@+id/filter_child_root"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_poi_gas_type_bg"
        >
        <com.sgm.navi.ui.view.SkinTextView
            android:id="@+id/poi_charge_label"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingStart="@dimen/dp_8"
            tools:text="地上"
            android:paddingEnd="@dimen/dp_8"
            android:textSize="@dimen/sp_24"
            android:visibility="gone"
            tools:visibility="visible"
            android:textColor="@color/traffic_sub_title_color"
            android:gravity="center"
            />
    </com.sgm.navi.ui.view.SkinConstraintLayout>
</layout>