<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="SceneSearchHistoryImpl"
            type="com.sgm.navi.scene.impl.search.SceneSearchHistoryImpl" />
    </data>

    <com.sgm.navi.ui.view.SkinConstraintLayout
        android:layout_width="@dimen/dp_545"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.sgm.navi.ui.view.SkinConstraintLayout
            android:id="@+id/scl_add0"
            clickCommand="@{()-> SceneSearchHistoryImpl.commonAdd(0)}"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_128"
            app:layout_constraintEnd_toStartOf="@id/scl_add1"
            app:layout_constraintStart_toStartOf="parent">

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/iv_add0"
                android:layout_width="@dimen/dp_49"
                android:layout_height="@dimen/dp_49"
                android:layout_marginTop="@dimen/dp_20"
                android:src="@drawable/img_basic_ic_add"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/tv_add0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxWidth="@dimen/dp_144"
                android:maxLines="1"
                android:text="@string/shv_common_address"
                android:textColor="@color/search_quick_tab_view_color"
                android:textSize="@dimen/sp_26"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/iv_add0" />
        </com.sgm.navi.ui.view.SkinConstraintLayout>

        <com.sgm.navi.ui.view.SkinConstraintLayout
            android:id="@+id/scl_add1"
            clickCommand="@{()-> SceneSearchHistoryImpl.commonAdd(1)}"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_149"
            app:layout_constraintEnd_toStartOf="@id/scl_add2"
            app:layout_constraintStart_toEndOf="@id/scl_add0">

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/iv_add1"
                android:layout_width="@dimen/dp_49"
                android:layout_height="@dimen/dp_49"
                android:layout_marginTop="@dimen/dp_20"
                android:src="@drawable/img_basic_ic_add"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/tv_add1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxWidth="@dimen/dp_144"
                android:maxLines="1"
                android:text="@string/shv_common_address"
                android:textColor="@color/search_quick_tab_view_color"
                android:textSize="@dimen/sp_26"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/iv_add1" />
        </com.sgm.navi.ui.view.SkinConstraintLayout>

        <com.sgm.navi.ui.view.SkinConstraintLayout
            android:id="@+id/scl_add2"
            clickCommand="@{()-> SceneSearchHistoryImpl.commonAdd(2)}"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_149"
            app:layout_constraintEnd_toStartOf="@id/scl_star"
            app:layout_constraintStart_toEndOf="@id/scl_add1">

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/iv_add2"
                android:layout_width="@dimen/dp_49"
                android:layout_height="@dimen/dp_49"
                android:layout_marginTop="@dimen/dp_20"
                android:src="@drawable/img_basic_ic_add"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/tv_add2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxWidth="@dimen/dp_144"
                android:maxLines="1"
                android:text="@string/shv_common_address"
                android:textColor="@color/search_quick_tab_view_color"
                android:textSize="@dimen/sp_26"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/iv_add2" />
        </com.sgm.navi.ui.view.SkinConstraintLayout>


        <com.sgm.navi.ui.view.SkinConstraintLayout
            android:id="@+id/scl_star"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_149"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/scl_add2">

            <com.sgm.navi.ui.view.SkinImageView
                android:id="@+id/iv_star"
                android:layout_width="@dimen/dp_49"
                android:layout_height="@dimen/dp_49"
                android:layout_marginTop="@dimen/dp_20"
                android:src="@drawable/img_basic_ic_star"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.sgm.navi.ui.view.SkinTextView
                android:id="@+id/tv_car_washing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:maxWidth="@dimen/dp_144"
                android:maxLines="1"
                android:text="@string/st_collect"
                android:textColor="@color/search_quick_tab_view_color"
                android:textSize="@dimen/sp_26"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/iv_star" />

        </com.sgm.navi.ui.view.SkinConstraintLayout>
    </com.sgm.navi.ui.view.SkinConstraintLayout>
</layout>