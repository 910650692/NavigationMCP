<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="@dimen/dp_603"
    android:layout_height="@dimen/dp_329"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingHorizontal="@dimen/dp_29"
    android:paddingVertical="@dimen/dp_29"
    android:background="@drawable/bg_shadow_navi_ndlc_8155"
    tools:ignore="MissingDefaultResource">

    <com.sgm.navi.ui.view.SkinImageView
        android:id="@+id/iv_loading"
        android:layout_width="@dimen/dp_68"
        android:layout_height="@dimen/dp_68"
        android:background="@drawable/icon_search_loading"
        android:layout_marginTop="@dimen/dp_75"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <com.sgm.navi.ui.view.SkinTextView
        android:id="@+id/stv_loading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/navi_color_000000_70"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="@string/tbt_loading"
        android:textSize="@dimen/sp_26"
        app:layout_constraintTop_toBottomOf="@+id/iv_loading"
        android:layout_marginTop="@dimen/dp_20"/>


</androidx.constraintlayout.widget.ConstraintLayout>